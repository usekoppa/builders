{
  "compilerOptions": {
    "incremental": true,
    "noEmit": false,
    "allowJs": false,
    "checkJs": false,
    "alwaysStrict": true,
    "declaration": false,
    "sourceMap": false,
    "importHelpers": true,
    "downlevelIteration": true,
    "noEmitHelpers": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["esnext"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "strictNullChecks": true,
    "target": "es2020",
    "skipLibCheck": true,
    "outDir": "./dist",
  },
  "include": ["src2/**/*"],
  "exclude": ["node_modules", "fail", "another_fail"]
}
